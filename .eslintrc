{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "eol-last": 0,
    "object-curly-spacing": [
      0,
      "never"
    ],
    "object-curly-newline": [
      "error",
      {
        "multiline": true,
        "minProperties": 4,
        "consistent": true
      }
    ],
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ]
  },
  "globals": {
    "window": true,
    "document": true,
    "fetch": false,
    "it": false,
    "expect": false
  }
}